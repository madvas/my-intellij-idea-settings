<application>
  <component name="ActionMacroManager">
    <macro name="clojure comment form">
      <typing text-keycode="51:1;45:1;32:0">#_&amp;#x20;</typing>
    </macro>
    <macro name="rerun last repl command">
      <action id=":cursive.repl.actions/load-file" />
      <action id="ActivateREPLToolWindow" />
      <action id=":cursive.repl.actions/prev-history-item" />
      <action id=":cursive.repl.actions/execute-immediately" />
    </macro>
    <macro name="rerun 1 before last REPL command">
      <action id="ActivateREPLToolWindow" />
      <action id=":cursive.repl.actions/prev-history-item" />
      <action id=":cursive.repl.actions/prev-history-item" />
      <action id=":cursive.repl.actions/execute-immediately" />
    </macro>
    <macro name="rerun last REPL command + switcher">
      <action id=":cursive.repl.actions/load-file" />
      <action id="ActivateREPLToolWindow" />
      <action id=":cursive.repl.actions/prev-history-item" />
      <action id=":cursive.repl.actions/execute-immediately" />
      <action id="Switcher" />
      <shortuct text="UP" />
      <shortuct text="ENTER" />
      <shortuct text="ENTER" />
      <shortuct text="ENTER" />
      <shortuct text="ENTER" />
      <shortuct text="ENTER" />
      <shortuct text="ENTER" />
      <shortuct text="ENTER" />
      <shortuct text="ENTER" />
    </macro>
    <macro name="rerun 1 before last command + switcher">
      <action id="ActivateREPLToolWindow" />
      <action id=":cursive.repl.actions/prev-history-item" />
      <action id=":cursive.repl.actions/prev-history-item" />
      <action id=":cursive.repl.actions/execute-immediately" />
      <action id="Switcher" />
      <shortuct text="UP" />
      <shortuct text="ENTER" />
      <shortuct text="ENTER" />
      <shortuct text="ENTER" />
      <shortuct text="ENTER" />
      <shortuct text="ENTER" />
      <shortuct text="ENTER" />
      <shortuct text="ENTER" />
    </macro>
    <macro name="u/p wrap">
      <action id=":cursive.actions.paredit/wrap-paren" />
      <typing text-keycode="85:0;47:0;80:0;32:0;222:1;72:0;69:0;82:0;69:0;59:1">u/p&amp;#x20;&quot;here:</typing>
      <action id="SaveAll" />
    </macro>
    <macro name="reset command repl">
      <action id="ActivateREPLToolWindow" />
      <typing text-keycode="57:1;82:0;69:0;83:0;69:0;84:0">(reset</typing>
      <action id=":cursive.repl.actions/execute-immediately" />
      <action id="Switcher" />
      <shortuct text="UP" />
      <shortuct text="ENTER" />
      <shortuct text="ENTER" />
      <shortuct text="ENTER" />
      <shortuct text="ENTER" />
      <shortuct text="ENTER" />
      <shortuct text="ENTER" />
      <shortuct text="ENTER" />
    </macro>
    <macro name="reset">
      <action id="ActivateREPLToolWindow" />
      <action id=":cursive.repl.actions/execute-immediately" />
      <typing text-keycode="57:1;82:0;69:0;83:0;69:0;84:0">(reset</typing>
      <action id=":cursive.repl.actions/execute-immediately" />
    </macro>
  </component>
</application>